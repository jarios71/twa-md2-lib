{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "twa-md2-dialogs": [
        "dist/twa-md2-dialogs"
      ],
      "twa-md2-dialogs/*": [
        "dist/twa-md2-dialogs/*"
      ],
      "twa-md2-notifications": [
        "dist/twa-md2-notifications"
      ],
      "twa-md2-notifications/*": [
        "dist/twa-md2-notifications/*"
      ],
      "twa-md2-filter-editor": [
        "dist/twa-md2-filter-editor"
      ],
      "twa-md2-filter-editor/*": [
        "dist/twa-md2-filter-editor/*"
      ]
    }
  }
}