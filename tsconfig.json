{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "twa-md2-dialogs": [
        "dist/twa-md2-dialogs"
      ],
      "twa-md2-dialogs/*": [
        "dist/twa-md2-dialogs/*"
      ],
      "twa-md2-notifications": [
        "dist/twa-md2-notifications"
      ],
      "twa-md2-notifications/*": [
        "dist/twa-md2-notifications/*"
      ],
      "twa-md2-filter-editor": [
        "dist/twa-md2-filter-editor"
      ],
      "twa-md2-filter-editor/*": [
        "dist/twa-md2-filter-editor/*"
      ]
    }
  }
}